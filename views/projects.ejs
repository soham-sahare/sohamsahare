<!DOCTYPE html>
<html lang="en" dir="ltr">
  <meta http-equiv="content-type" content="text/html;charset=utf-8" />
  <head>
    <title>Projects - sohamsahare</title>
    <%- include('partials/head') %>
  </head>
  <body>
    <%- include('partials/loader') %>
    <%- include('partials/navbar') %>
    <%- include('partials/header') %>


    
    <main id="ss-mxd-page-content" class="ss-mxd-page-content inner-page-content">

      
      <div class="ss-mxd-section ss-mxd-section-inner-headline padding-headline-pre-stack">
        <div class="ss-mxd-container grid-container">
        
          
          <div class="ss-mxd-block loading-wrap">
            <div class="container-fluid px-0">
              <div class="row gx-0">

                
                <div class="col-12 col-xl-2 ss-mxd-grid-item no-margin">
                  <div class="ss-mxd-block__name name-inner-headline loading__item">
                    <p class="ss-mxd-point-subtitle">
                      <svg version="1.1" xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="20px" height="20px" viewBox="0 0 20 20" fill="currentColor">
                        <path fill="currentColor" d="M19.6,9.6c0,0-3,0-4,0c-0.4,0-1.8-0.2-1.8-0.2c-0.6-0.1-1.1-0.2-1.6-0.6c-0.5-0.3-0.9-0.8-1.2-1.2
                          c-0.3-0.4-0.4-0.9-0.5-1.4c0,0-0.1-1.1-0.2-1.5c-0.1-1.1,0-4.4,0-4.4C10.4,0.2,10.2,0,10,0S9.6,0.2,9.6,0.4c0,0,0.1,3.3,0,4.4
                          c0,0.4-0.2,1.5-0.2,1.5C9.4,6.7,9.2,7.2,9,7.6C8.7,8.1,8.2,8.5,7.8,8.9c-0.5,0.3-1,0.5-1.6,0.6c0,0-1.2,0.1-1.7,0.2
                          c-1,0.1-4.2,0-4.2,0C0.2,9.6,0,9.8,0,10c0,0.2,0.2,0.4,0.4,0.4c0,0,3.1-0.1,4.2,0c0.4,0,1.7,0.2,1.7,0.2c0.6,0.1,1.1,0.2,1.6,0.6
                          c0.4,0.3,0.8,0.7,1.1,1.1c0.3,0.5,0.5,1,0.6,1.6c0,0,0.1,1.3,0.2,1.7c0,1,0,4.1,0,4.1c0,0.2,0.2,0.4,0.4,0.4s0.4-0.2,0.4-0.4
                          c0,0,0-3.1,0-4.1c0-0.4,0.2-1.7,0.2-1.7c0.1-0.6,0.2-1.1,0.6-1.6c0.3-0.4,0.7-0.8,1.1-1.1c0.5-0.3,1-0.5,1.6-0.6
                          c0,0,1.3-0.1,1.8-0.2c1,0,4,0,4,0c0.2,0,0.4-0.2,0.4-0.4C20,9.8,19.8,9.6,19.6,9.6L19.6,9.6z"/>
                      </svg>
                      <span>Projects</span>
                    </p>
                  </div>
                </div>
                

                
                <div class="col-12 col-xl-10 ss-mxd-grid-item no-margin">

                  <div class="ss-mxd-block__content">
                    <div class="ss-mxd-block__inner-headline loading__item">
                      <h1 class="inner-headline__title headline-img-after headline-img-03">Where bold ideas take shape</h1>
                      
                    </div>
                  </div>

                </div>
                

              </div>
            </div>
          </div>
          

        </div>
      </div>
      

      
      <div class="ss-mxd-section padding-stacked-section">
        <div class="ss-mxd-container grid-container">

          
          <div class="ss-mxd-block ss-mxd-grid-item no-margin">
            <div class="content__block loading__fade">
              <div class="stack-wrapper">
                <div class="stack-offset"></div>
                <div class="projects-stack">
                  <% if (projects.length === 0) { %>
                    <div class="empty-state" style="text-align: center; padding: 4rem 2rem; color: var(--t-muted);">
                      <i class="ph ph-folder-plus" style="font-size: 4rem; margin-bottom: 1rem; color: var(--t-muted-extra);"></i>
                      <h3 style="margin-bottom: 1rem;">No Projects Yet</h3>
                      <p>Projects will be available soon</p>
                    </div>
                  <% } else { %>
                    <% projects.slice(0, 3).forEach((project, index) => { %>
                    <div class="stack-item">
                      <a class="ss-mxd-projects-stack__inner justify-between" href="/projects/<%= project.id %>">
                        <div class="ss-mxd-projects-stack__image">
                          <img src="<%= project.images[0] %>" alt="<%= project.title %> Preview">
                        </div>
                        <div class="ss-mxd-projects-stack__tags">
                          <% 
                            const tags = Array.isArray(project.tags) ? project.tags : (project.tags ? project.tags.split(',').map(t => t.trim()) : []);
                            tags.slice(0, 3).forEach(tag => { 
                          %>
                            <span class="tag tag-default tag-outline-permanent"><%= tag %></span>
                          <% }); %>
                        </div>
                        <div class="ss-mxd-projects-stack__title no-margin">
                          <h2 class="permanent-light"><%= project.title %></h2>
                        </div>
                      </a>
                    </div>
                    <% }); %>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
          

        </div>
      </div>
      

      
      <div class="ss-mxd-section overflow-hidden padding-grid-pre-mtext">
        <div class="ss-mxd-container grid-container">

          
          <div class="ss-mxd-block">
            <div class="ss-mxd-section-title">
              <div class="container-fluid p-0">
                <div class="row g-0">
                  <div class="col-12 col-xl-6 ss-mxd-grid-item no-margin">
                    <div class="ss-mxd-section-title__hrtitle">
                      <h2 class="reveal-type anim-uni-in-up">Projects archive</h2>
                    </div>
                  </div>
                  <div class="col-12 col-xl-2 ss-mxd-grid-item no-margin"></div>
                  <div class="col-12 col-xl-4 ss-mxd-grid-item no-margin">
                    <div class="ss-mxd-section-title__hrcontrols pre-title anim-uni-in-up">
                      <a class="btn btn-anim btn-default btn-outline slide-right-down" href="/about-me#testimonials">
                        <span class="btn-caption">Testimonials</span>
                        <i class="ph-bold ph-arrow-down-right"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          

          
          <div class="ss-mxd-block">
            <% if (projects.length > 3) { %>
              <div class="ss-mxd-projects-list hover-reveal">
                <% projects.slice(3).forEach((project, index) => { %>
                  <a class="ss-mxd-projects-list__item hover-reveal__item" href="/projects/<%= project.id %>">
                    <div class="ss-mxd-projects-list__border anim-uni-in-up"></div>
                    <div class="hover-reveal__content hover-reveal-280x340">
                      <img class="hover-reveal__image" src="<%= project.images[0] %>" alt="<%= project.title %> Preview">
                    </div>
                    <div class="ss-mxd-projects-list__inner">
                      <div class="container-fluid px-0">
                        <div class="row gx-0">
                          <div class="col-12 col-xl-8 ss-mxd-grid-item no-margin">
                            <div class="ss-mxd-projects-list__title anim-uni-in-up">
                              <div class="ss-mxd-projects-list__icon">
                                <i class="ph ph-arrow-right"></i>
                              </div>
                              <p><%= project.title %></p>
                            </div>
                            <div class="ss-mxd-projects-list__image anim-uni-in-up">
                              <img src="<%= project.images[1] || project.images[0] %>" alt="<%= project.title %> Preview">
                            </div>
                          </div>
                          <div class="col-6 col-md-6 col-xl-2 ss-mxd-grid-item no-margin">
                            <div class="ss-mxd-projects-list__tagslist">
                              <ul>
                                <% 
                                  const tags = Array.isArray(project.tags) ? project.tags : (project.tags ? project.tags.split(',').map(t => t.trim()) : []);
                                  tags.slice(0, 2).forEach(tag => { 
                                %>
                                  <li class="anim-uni-in-up">
                                    <p class="t-small"><%= tag %></p>
                                  </li>
                                <% }); %>
                              </ul>
                            </div>
                          </div>
                          <div class="col-6 col-md-6 col-xl-2 ss-mxd-grid-item no-margin">
                            <div class="ss-mxd-projects-list__date anim-uni-in-up">
                              <p class="t-small"><%= project.date %></p>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="ss-mxd-projects-list__border anim-uni-in-up"></div>
                  </a>
                <% }); %>
              </div>
            <% } else { %>
              <div class="ss-mxd-block__content">
                <div class="ss-mxd-block__manifest centered anim-uni-in-up" style="padding: 60px 0;">
                  <div class="ss-mxd-manifest__icon" style="margin-bottom: 30px;">
                    <i class="ph ph-folder-plus" style="font-size: 48px; color: #666;"></i>
                  </div>
                  <h2 class="ss-mxd-manifest ss-mxd-manifest-l reveal-type" style="margin-bottom: 20px;">More Projects Coming Soon</h2>
                  
                  <div class="ss-mxd-manifest__controls anim-uni-in-up" style="margin-top: 40px;">
                    <div class="ss-mxd-btngroup centered">
                      <a class="btn btn-anim btn-default btn-accent slide-right-up" href="/contact">
                        <span class="btn-caption">Get in Touch</span>
                        <i class="ph-bold ph-arrow-up-right"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            <% } %>
          </div>
          

        </div>
      </div>
      

      
      <div class="ss-mxd-section overflow-hidden">
        <div class="ss-mxd-container">

          
          <div class="ss-mxd-block">
            <div class="ss-mxd-promo">
              <div class="ss-mxd-promo__inner anim-zoom-out-container">
                
                <div class="ss-mxd-promo__bg"></div>
                
                <div class="ss-mxd-promo__content">
                  <p class="ss-mxd-promo__title anim-uni-in-up">
                    <span class="ss-mxd-promo__icon">
                      <img src="img/icons/300x300_obj-cta-01.webp" alt="Icon">
                    </span>
                    <span class="ss-mxd-promo__caption reveal-type">Let's talk about your project!</span>
                  </p>
                  <div class="ss-mxd-promo__controls anim-uni-in-up">
                    <a class="btn btn-anim btn-default btn-large btn-additional slide-right-up" href="/contact">
                      <span class="btn-caption">Contact Us</span>
                      <i class="ph-bold ph-arrow-up-right"></i>
                    </a>
                  </div>
                </div>
                
                <div class="ss-mxd-promo__images">
                  <img class="promo-image promo-image-1" src="img/illustrations/cta-img-01.webp" alt="Image">
                  <img class="promo-image promo-image-2" src="img/illustrations/cta-img-02.webp" alt="Image">
                </div>
              </div>
            </div>
          </div>
          

        </div>
      </div>
      

    </main>

    <%- include('partials/footer') %>

    <a href="#0" id="to-top" class="btn btn-to-top slide-up anim-no-delay">
        <i class="ph ph-arrow-up"></i>
    </a>

    <script src="js/libs.min.js"></script>
    <script src="js/app.min.js"></script>
    
  </body>
</html>